<!DOCTYPE html>
<html lang="pl">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>CaseDrop | Anubis Case</title>
		<link rel="preconnect" href="https://fonts.googleapis.com" />
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
		<link
			href="https://fonts.googleapis.com/css2?family=Wix+Madefor+Display:wght@400;700&display=swap"
			rel="stylesheet"
		/>
		<link rel="stylesheet" href="./dist/css/style.min.css" />
		<script
			src="https://kit.fontawesome.com/bdb2f8ae99.js"
			crossorigin="anonymous"
		></script>
	</head>
	<body>
		<!-- @include "nav.kit" -->
		<div class="casebox">
			<div class="casebox__caseimg">
				<img
					class="casebox__caseimg-img"
					src="./dist/img/anubis/case.png"
					alt="Anubis Case"
				/>
			</div>
			<div class="casebox-arrows">
				<div class="casebox-arrows-arrow casebox-arrows-arrow--down">
					<i class="casebox-arrow-down-icon fa-solid fa-arrow-down"></i>
				</div>
				<div class="casebox-arrows-arrow casebox-arrows-arrow--up">
					<i class="casebox-arrow-up-icon fa-solid fa-arrow-up"></i>
				</div>
			</div>
			<ul class="case"></ul>
		</div>
		<div class="open-btn-box">
			<button class="open-btn">
				open
				<img
					class="open-btn-icon"
					src="./dist/img/other/coin.png"
					alt="Coin Icon"
				/>
				<p class="open-btn-amount">5.00</p>
			</button>
		</div>
		<!-- <button class="drops-btn">See Drop</button> -->
		<!-- <div class="drops hidden">
			<div class="drops__bg"></div>
			<ul class="drops__list"></ul>
			<button class="drops__close">close</button>
		</div> -->
		<div class="winning-item hidden">
			<img src="./dist/img/anubis/apep'scurse.png" alt="" />
			<button class="winning-item-button">close</button>
			<!-- <button class="winning-item-button winning-item-button--upgrade">
				upgrade
			</button> -->
		</div>
		<!-- <div class="upgrader hidden">
			<div class="bg"></div>
			<div class="upgrader__text">
				<p class="upgrader__text-item">Your balance:</p>
				<p class="upgrader__text-chance">?%</p>
				<p class="upgrader__text-amount">Chance to win:</p>
			</div> -->
		<!-- <button class="upgrader__start">upgrade</button>
			<input class="amount" type="number" placeholder="Put amount"> -->
		<!-- </div> -->
		<script src="./dist/js/caseanubis.min.js"></script>
	</body>
</html>
