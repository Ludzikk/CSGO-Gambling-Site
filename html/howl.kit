<!DOCTYPE html>
<html lang="en">
	<head>
		<!-- @include "meta.kit" -->
		<title>CSGOLuck | Howl Case</title>
		<!-- @include "link.kit" -->
	</head>
	<body>
		<!-- @include "nav.kit" -->
		<div class="casebox" data-aos="fade-in" data-aos-duration="500">
			<div class="casebox__caseimg">
				<img
					class="casebox__caseimg-img"
					src="./dist/img/knife5050/case.png"
					alt="Anubis Case"
				/>
			</div>
			<div class="casebox-arrows">
				<div class="casebox-arrows-arrow casebox-arrows-arrow--down">
					<i class="casebox-arrow-down-icon fa-solid fa-arrow-down"></i>
				</div>
				<div class="casebox-arrows-arrow casebox-arrows-arrow--up">
					<i class="casebox-arrow-up-icon fa-solid fa-arrow-up"></i>
				</div>
			</div>
			<ul class="case"></ul>
		</div>
		<div class="open-btn-box" data-aos="fade-in" data-aos-duration="500">
			<button class="open-btn">
				open
				<img
					class="open-btn-icon"
					src="./dist/img/other/coin.png"
					alt="Coin Icon"
				/>
				<p class="open-btn-amount">2600.00</p>
			</button>
		</div>
		<div class="chances" data-aos="fade-in" data-aos-duration="500">
			<ul class="chances__list">
				<li class="chances__list-item chances__list-item--red">
					<img
						src="./dist/img/howl/howl.png"
						alt="M4A4 | Howl"
						class="chances__list-item-img"
					/>
					<p class="chances__list-item-text">50%</p>
					<div class="chances__list-item-price hidden">
						<p class="chances__list-item-price-text">5213</p>
                        <img class="chances__list-item-price-icon" src="./dist/img/other/coin.png" alt="Coin Icon">
					</div>
				</li>
				<li class="chances__list-item
				chances__list-item--blue">
					<img
						src="./dist/img/howl/polymag.png"
						alt="M4A4 | Poly Mag"
						class="chances__list-item-img"
					/>
					<p class="chances__list-item-text">50%</p>
					<div class="chances__list-item-price hidden">
						<p class="chances__list-item-price-text">0.68</p>
                        <img class="chances__list-item-price-icon" src="./dist/img/other/coin.png" alt="Coin Icon">
					</div>
				</li>
			</ul>
		</div>
		<!-- @include "footer.kit" -->
		<script src="./dist/js/casehowl.min.js"></script>
		<script src="./dist/js/casechances.min.js"></script>
		<script src="./dist/js/footeryear.min.js"></script>
		<script src="./dist/js/setbalance.min.js"></script>
		<script src="./dist/js/nav.min.js"></script>
		<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
		<script>
			AOS.init();
		  </script>
	</body>
</html>
