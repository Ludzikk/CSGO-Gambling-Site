<!DOCTYPE html>
<html lang="en">
	<head>
		<!-- @include "meta.kit" -->
		<title>CSGOLuck | Cases</title>
		<!-- @include "link.kit" -->
	</head>
	<body>
		<!-- @include "nav.kit" -->
		<main class="main-cases" data-aos="fade-in" data-aos-duration="500" data-aos-once="true">
			<ul class="main-cases__list">
				<li class="main-cases__list-item">
					<div class="images">
						<img
							src="./dist/img/anubis/eyeofhorus.png"
							alt="M4A4 Eye of Horus"
							class="main-cases__list-item-photo main-cases__list-item-photo--item hidden"
						/>
						<img
							src="./dist/img/anubis/case.png"
							alt="Anubis Case"
							class="main-cases__list-item-photo main-cases__list-item-photo--case"
						/>
					</div>
					<p class="main-cases__list-item-name">Anubis</p>
					<p class="main-cases__list-item-price">
						<img src="./dist/img/other/coin.png" alt="Coin Icon" class="main-cases__list-item-price-img">
						5.00
					</p>
					<a class="main-cases__list-item-button" href="anubis.html">Open</a>
				</li>
				<li class="main-cases__list-item">
					<div class="images">
						<img
							src="./dist/img/printstream/m4a1-sprintstream.png"
							alt="M4A1-S Printstream"
							class="main-cases__list-item-photo main-cases__list-item-photo--item hidden"
						/>
						<img
							src="./dist/img/printstream/case.png"
							alt="Anubis Case"
							class="main-cases__list-item-photo main-cases__list-item-photo--case"
						/>
					</div>
					<p class="main-cases__list-item-name">Printstream</p>
					<p class="main-cases__list-item-price">
						<img src="./dist/img/other/coin.png" alt="Coin Icon" class="main-cases__list-item-price-img">
						20.00
					</p>
					<a class="main-cases__list-item-button" href="printstream.html">Open</a>
				</li>
				<li class="main-cases__list-item">
					<div class="images">
						<img
							src="./dist/img/awp/dragonlore.png"
							alt="AWP Dragonlore"
							class="main-cases__list-item-photo main-cases__list-item-photo--item hidden"
						/>
						<img
							src="./dist/img/awp/case.png"
							alt="AWP Case"
							class="main-cases__list-item-photo main-cases__list-item-photo--case"
						/>
					</div>
					<p class="main-cases__list-item-name">awp case</p>
					<p class="main-cases__list-item-price">
						<img src="./dist/img/other/coin.png" alt="Coin Icon" class="main-cases__list-item-price-img">
						2000.00
					</p>
					<a class="main-cases__list-item-button" href="awpcase.html">Open</a>
				</li>
				<li class="main-cases__list-item">
					<div class="images">
						<img
							src="./dist/img/knife5050/doppler.png"
							alt="Karambit Doppler"
							class="main-cases__list-item-photo main-cases__list-item-photo--item hidden"
						/>
						<img
							src="./dist/img/knife5050/case.png"
							alt="AWP Case"
							class="main-cases__list-item-photo main-cases__list-item-photo--case"
						/>
					</div>
					<p class="main-cases__list-item-name">knife 50/50</p>
					<p class="main-cases__list-item-price">
						<img src="./dist/img/other/coin.png" alt="Coin Icon" class="main-cases__list-item-price-img">
						800
					</p>
					<a class="main-cases__list-item-button" href="knife5050.html">Open</a>
				</li>
				<li class="main-cases__list-item">
					<div class="images">
						<img
							src="./dist/img/howl/howl.png"
							alt="M4A4 | Howl"
							class="main-cases__list-item-photo main-cases__list-item-photo--item hidden"
						/>
						<img
							src="./dist/img/howl/case.png"
							alt="Howl Case"
							class="main-cases__list-item-photo main-cases__list-item-photo--case"
						/>
					</div>
					<p class="main-cases__list-item-name">Howl Case</p>
					<p class="main-cases__list-item-price">
						<img src="./dist/img/other/coin.png" alt="Coin Icon" class="main-cases__list-item-price-img">
						2600
					</p>
					<a class="main-cases__list-item-button" href="howl.html">Open</a>
				</li>
				<li class="main-cases__list-item">
					<div class="images">
						<img
							src="./dist/img/glove/slingshot.png"
							alt="Sport Gloves | Slingshot"
							class="main-cases__list-item-photo main-cases__list-item-photo--item hidden"
						/>
						<img
							src="./dist/img/glove/case.png"
							alt="Glove Case"
							class="main-cases__list-item-photo main-cases__list-item-photo--case"
						/>
					</div>
					<p class="main-cases__list-item-name">Glove Case</p>
					<p class="main-cases__list-item-price">
						<img src="./dist/img/other/coin.png" alt="Coin Icon" class="main-cases__list-item-price-img">
						200
					</p>
					<a class="main-cases__list-item-button" href="glovecase.html">Open</a>
				</li>
			</ul>
		</main>
		<div class="main">
			<aside class="main__drops" data-aos="fade-in" data-aos-duration="500">
				<div class="main__top">
					<p class="main__top-text">Last Drops</p>
				</div>
				<div class="main__dropsbox">
				</div>
			</aside>
		</div>
		<div class="main-chat">
			<aside class="main-chat__chatbox" data-aos="fade-in" data-aos-duration="500">
				<div class="main-chat__messages">
				</div>
				<div class="main-chat__inputbox">
					<input type="text" class="main-chat__input" placeholder="Send msg...">
					<button class="main-chat__button">Send</button>
				</div>
			</aside>
		</div>
		<!-- @include "footer.kit" -->
		<script src="./dist/js/cases.min.js"></script>
		<script src="./dist/js/setbalance.min.js"></script>
		<script src="./dist/js/footeryear.min.js"></script>
		<script src="./dist/js/nav.min.js"></script>
		<script src="./dist/js/lastdrops.min.js"></script>
		<script src="./dist/js/chat.min.js"></script>
		<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
		<script>
			AOS.init();
		  </script>
	</body>
</html>
