<!DOCTYPE html>
<html lang="pl">
	<head>
		<!-- @include "meta.kit" -->
		<title>CSGOLuck | CoinFlip</title>
		<!-- @include "link.kit" -->
	</head>
	<body>
		<!-- @include "nav.kit" -->
		<main class="main-coinflip" data-aos="fade-in" data-aos-duration="500" data-aos-once="true">
			<div class="main-coinflip__coin">
				<div class="main-coinflip__red">
					<img
						src="./dist/img/other/coin.png"
						alt="Coin Icon"
						class="main-coinflip__img"
					/>
				</div>
				<div class="main-coinflip__black">
					<img
						src="./dist/img/other/coin.png"
						alt="Coin Icon"
						class="main-coinflip__img"
					/>
				</div>
			</div>
			<div class="main-coinflip__betbox">
				<div class="main-coinflip__inputbox">
					<input
						class="main-coinflip__input"
						type="number"
						placeholder="Place bet..." value="0"
						min="0"
					/>
					<div
						class="main-coinflip__inputbox-button main-coinflip__inputbox-button--clear"
						id="clear"
					>
						Clear
					</div>
					<div class="main-coinflip__inputbox-button" id="last">Last</div>
					<div class="main-coinflip__inputbox-button" id="plus1">+1</div>
					<div class="main-coinflip__inputbox-button" id="plus10">+10</div>
					<div class="main-coinflip__inputbox-button" id="plus100">+100</div>
					<div class="main-coinflip__inputbox-button" id="plus1000">+1000</div>
					<div class="main-coinflip__inputbox-button" id="half">1/2</div>
					<div class="main-coinflip__inputbox-button" id="x2">x2</div>
					<div class="main-coinflip__inputbox-button" id="max">max</div>
				</div>
			</div>
			<div class="main-coinflip__betbuttons">
				<button class="main-coinflip__button main-coinflip__button--red">
					<img
						src="./dist/img/other/coin.png"
						alt="Coin Icon"
						class="main-coinflip__betimg"
					/>
				</button>
				<button class="main-coinflip__button main-coinflip__button--black">
					<img
						src="./dist/img/other/coin.png"
						alt="Coin Icon"
						class="main-coinflip__betimg"
					/>
				</button>
			</div>
		</main>
		<div class="main">
			<aside class="main__drops" data-aos="fade-in" data-aos-duration="500">
				<div class="main__top">
					<p class="main__top-text">Last Drops</p>
				</div>
				<div class="main__dropsbox">
				</div>
			</aside>
		</div>
		<div class="main-chat">
			<aside class="main-chat__chatbox" data-aos="fade-in" data-aos-duration="500">
				<div class="main-chat__messages">
				</div>
				<div class="main-chat__inputbox">
					<input type="text" class="main-chat__input" placeholder="Send msg...">
					<button class="main-chat__button">Send</button>
				</div>
			</aside>
		</div>
		<!-- @include "footer.kit" -->
		<script src="./dist/js/coinflip.min.js"></script>
		<script src="./dist/js/footeryear.min.js"></script>
		<script src="./dist/js/setbalance.min.js"></script>
		<script src="./dist/js/lastdrops.min.js"></script>
		<script src="./dist/js/chat.min.js"></script>
		<script src="./dist/js/nav.min.js"></script>
		<script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
		<script>
			AOS.init();
		</script>
	</body>
</html>
